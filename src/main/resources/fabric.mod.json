  "authors": [
    "Identity Theft"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT License",
  "icon": "assets/sculkitems/icon.png",

  "environment": "*",
